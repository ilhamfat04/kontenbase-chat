{
  "name": "kontenbase/sdk",
  "description": "Kontenbase SDK for PHP",
  "type": "library",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://kontenbase.com",
  "authors": [
    {
      "name": "Kontenbase",
      "homepage": "https://kontenbase.com"
    }
  ],
  "config": {
    "vendor-dir": "packages/sdk/php/vendor"
  },
  "readme": "packages/sdk/php/README.md",
  "scripts": {
    "test": "phpunit packages/sdk/php/tests"
  },
  "autoload": {
    "psr-4": {
      "Kontenbase\\": "packages/sdk/php/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kontenbase\\Tests\\": "packages/sdk/php/tests/"
    }
  },
  "require": {
    "guzzlehttp/guzzle": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}